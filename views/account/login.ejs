<h1><%= title %></h1>
<p><%= messages() %></p>

    <div id="rese">
      <form action="/account/login" method="post">
        <label for="email">Email:</label>
        <input type="email" id="email" name="account_email" required>
      <div class="pasi">
      <label for="password">Password:</label>
      <input type="password" id="password" name="account_password" required>
      <p>Passwords must be minimum of 12 characters and include 1 capital letter, 1 number and 1 special character.</p>
      <div id="pswdBtn">Show Password</div>
          <button type="submit">LOGIN</button>
          <p>No account?<a href="/account/register"><strong>Sign Up</strong></a></p>
        </div>
      </form>
      <script src="/js/script.js"></script>
    </div>



